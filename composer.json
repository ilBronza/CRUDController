{
    "name": "ilbronza/crud",
    "description": ":crud_manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bronzini",
            "email": "davide_bronzini@hotmail.com",
            "homepage": "https://sistema.srl"
        }
    ],
    "homepage": "https://github.com/ilbronza/crud",
    "keywords": ["Laravel", "CRUD"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "spatie/laravel-permission": "^3.5"        
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ilBronza\\CRUD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ilBronza\\CRUD\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ilBronza\\CRUD\\CRUDServiceProvider"
            ],
            "aliases": {
                "CRUD": "ilBronza\\CRUD\\Facades\\CRUD"
            }
        }
    }
}
